{
  "name": "cookick",
  "version": "0.2.0",
  "description": "Utilities for cookie APIs",
  "author": "qiqiboy",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8.0.0",
    "tiger-new": "4.2.17"
  },
  "entryFile": "src/index.ts",
  "exportName": "cookick",
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "npm run lint && npm run clear && npm run build:declaration && npm run build:bundle",
    "build:bundle": "rollup -c",
    "build:declaration": "tsc --emitDeclarationOnly",
    "clear": "rimraf dist",
    "lint": "node_modules/.bin/eslint 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "node_modules/.bin/commitlint --edit $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ],
      "scope-case": [
        0
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none",
    "jsxBracketSameLine": true,
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,mjs,ts,tsx}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss,less,json,html,md}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  },
  "peerDependencies": {
    "@babel/runtime": ">7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://qiqiboy@github.com/qiqiboy/cookick.git"
  },
  "keywords": [
    "cookie",
    "browser-cookie",
    "typescript",
    "cookie-utilities",
    "cookie-lib",
    "cookie-tool"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@rollup/plugin-node-resolve": "6.0.0",
    "@rollup/plugin-replace": "2.2.1",
    "@types/node": "11.15.3",
    "@types/react": "16.9.14",
    "@types/react-dom": "16.9.4",
    "@types/react-is": "16.7.1",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "babel-eslint": "10.0.3",
    "babel-preset-react-app": "9.0.2",
    "eslint": "6.7.2",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rimraf": "2.6.3",
    "rollup": "1.27.8",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "1.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-sass": "1.2.2",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "4.0.4",
    "typescript": "3.7.2"
  }
}
